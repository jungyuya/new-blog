{
  "name": "new-blog",
  "private": true,
  "scripts": {
    "build": "turbo run build --filter=frontend",
    "dev": "concurrently -k --names \"FRONTEND,BACKEND\" -c \"bgBlue.bold,bgMagenta.bold\" \"pnpm --filter frontend dev\" \"pnpm --filter backend dev\"",
    "lint": "turbo run lint",
    "clean": "turbo run clean"
  },
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "concurrently": "^9.2.0",
    "turbo": "2.5.5",
    "typescript": "^5.5.4"
  },
  "pnpm": {
    "overrides": {
      "@typescript-eslint/eslint-plugin": "^8.0.0-alpha.54",
      "@typescript-eslint/parser": "^8.0.0-alpha.54"
    }
  },
  "turbo": {
    "teamId": "jungyu-dev",
    "remoteCache": {
      "signature": true
    }
  }
}