{
  "rules": [
    {
      "rulePriority": 1,
      "description": "Expire cache-tagged images older than 1 day",
      "selection": {
        "tagStatus": "tagged",
        "tagPrefixList": ["cache"],
        "countType": "sinceImagePushed",
        "countUnit": "days",
        "countNumber": 1
      },
      "action": {
        "type": "expire"
      }
    },
    {
      "rulePriority": 2,
      "description": "Expire untagged images older than 1 day",
      "selection": {
        "tagStatus": "untagged",
        "countType": "sinceImagePushed",
        "countUnit": "days",
        "countNumber": 1
      },
      "action": {
        "type": "expire"
      }
    }
  ]
}
