#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jungyu/projects/new-blog/node_modules/.pnpm/aws-cdk@2.1023.0/node_modules/aws-cdk/bin/node_modules:/home/jungyu/projects/new-blog/node_modules/.pnpm/aws-cdk@2.1023.0/node_modules/aws-cdk/node_modules:/home/jungyu/projects/new-blog/node_modules/.pnpm/aws-cdk@2.1023.0/node_modules:/home/jungyu/projects/new-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jungyu/projects/new-blog/node_modules/.pnpm/aws-cdk@2.1023.0/node_modules/aws-cdk/bin/node_modules:/home/jungyu/projects/new-blog/node_modules/.pnpm/aws-cdk@2.1023.0/node_modules/aws-cdk/node_modules:/home/jungyu/projects/new-blog/node_modules/.pnpm/aws-cdk@2.1023.0/node_modules:/home/jungyu/projects/new-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../aws-cdk/bin/cdk" "$@"
else
  exec node  "$basedir/../aws-cdk/bin/cdk" "$@"
fi
