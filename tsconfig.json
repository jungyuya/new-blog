{
  "files": [],
  "references": [
    { "path": "./apps/frontend" },
    { "path": "./apps/backend" },
    { "path": "./apps/frontend/amplify" }
  ],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["es2022", "DOM", "DOM.Iterable"],
    "baseUrl": ".",
    "paths": {
      "@aws-amplify/*": ["./node_modules/@aws-amplify/*"],
      "aws-amplify": ["./node_modules/aws-amplify"],
      "aws-cdk-lib": ["./node_modules/aws-cdk-lib"],
      "constructs": ["./node_modules/constructs"],
      "@aws-sdk/*": ["./node_modules/@aws-sdk/*"],
      "next": ["./node_modules/next"],
      "next/*": ["./node_modules/next/*"],
      "react": ["./node_modules/react"],
      "react-dom": ["./node_modules/react-dom"],
      "uuid": ["./node_modules/uuid"],
      "@types/*": ["./node_modules/@types/*"],
      "eslint": ["./node_modules/eslint"],
      "eslint-config-next": ["./node_modules/eslint-config-next"],
      "tailwindcss": ["./node_modules/tailwindcss"],
      "postcss": ["./node_modules/postcss"],
      "autoprefixer": ["./node_modules/autoprefixer"],
      "react-hook-form": ["./node_modules/react-hook-form"]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules"
    ]
  }
}